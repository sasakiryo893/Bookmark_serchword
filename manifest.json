{
  "name": "Bookmark_searchword",
  "description": "検索ワードを含めたブックマーク登録",
  "version": "1.0",
  "content_scripts": [{
    "css": ["css/popupWindow.css","css/popup.css"],
    "js": ["js/jquery-3.3.1.min.js", "js/background.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_icon": "sources/icon.png",
    "default_popup": "popup.html",
    "default_title": "ブックマーク"
  },
  "manifest_version": 2,
  "background": {
    "scripts": ["js/jquery-3.3.1.min.js","js/background.js"]
  },
  "permissions": ["tabs","history"]
}
