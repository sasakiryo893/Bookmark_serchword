{
  "name": "拡張機能の名前",
  "description": "拡張機能の説明",
  "version": "1.0",
  "content_scripts": [{
    "css": ["css/popupWindow.css","css/popup.css"],
    "js": ["js/jquery-3.3.1.min.js", "js/background.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ツールバーのアイコンにマウスオーバーで表示されるツールチップ"
  },
  "manifest_version": 2,
  "background": {
    "scripts": ["js/jquery-3.3.1.min.js","js/background.js"]
  },
  "permissions": ["tabs","history"]
}
